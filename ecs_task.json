{
    "containerDefinitions": [
        {
            "name": "cv-sample-app",
            "image": "<+artifact.image>",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "cv-sample-app-8082-tcp",
                    "containerPort": 8082,
                    "hostPort": 8082,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "prometheus.io/path",
                    "value": "/actuator/prometheus"
                },
                {
                    "name": "prometheus.io/port",
                    "value": "8082"
                },
                {
                    "name": "prometheus.io/scrape",
                    "value": "true"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": []
        }
    ],
    "family": "cv-sample-app",
    "taskRoleArn": "arn:aws:iam::123456role/ecsInstanceRole",
    "executionRoleArn": "arn:aws:iam::123456role:role/ecsInstanceRole",
    "networkMode": "awsvpc",
    "revision": 1,
    "volumes": [],
    "status": "ACTIVE",
    "requiresAttributes": [
        {
            "name": "com.amazonaws.ecs.capability.ecr-auth"
        },
        {
            "name": "com.amazonaws.ecs.capability.task-iam-role"
        },
        {
            "name": "ecs.capability.execution-role-ecr-pull"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
        },
        {
            "name": "ecs.capability.task-eni"
        }
    ],
    "placementConstraints": [],
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
